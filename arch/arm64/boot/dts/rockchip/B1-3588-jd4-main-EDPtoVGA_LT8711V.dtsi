 / {

    // VCC5V0_HOST1_EN: VCC5V0_HOST1_EN {
    //     compatible = "regulator-fixed";
    //     enable-active-high;
    //     gpio =  <&gpio1 RK_PC3 GPIO_ACTIVE_HIGH>;

