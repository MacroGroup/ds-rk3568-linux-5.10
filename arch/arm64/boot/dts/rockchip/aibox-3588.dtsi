 #define rk3588
 #include "rk3588-firefly-port.dtsi"
 #include "rk3588-diff.dtsi"

#define M2_SATA_OR_PCIE 0 /*1 = SATA , 0 = PCIe */

 / {
/* led */
	firefly_leds: leds {
		pinctrl-names = "default";
		pinctrl-0 =<&leds_gpio>;
		power_led: power {
			gpios = <&gpio2 RK_PC3 GPIO_ACTIVE_HIGH>;//blue led
		};

		user_led: user {
			gpios = < &gpio3 RK_PC1 GPIO_ACTIVE_HIGH>;//red led
		};

		user1_led: user1 {
			label = ":user1"; 
			default-state = "off";
			gpios = <&gpio3 RK_PC0 GPIO_ACTIVE_HIGH>;//green led
		};
	};

 	fan: pwm-fan {
		compatible = "pwm-fan";
		#cooling-cells = <2>;
		fan-supply = <&vcc5v0_sys>;
		pwms = <&pwm5 0 50000 0>;
		cooling-levels = <0 50 100 150 200 255>;
        rockchip,temp-trips = <
            50000   1
            55000   2
            60000   3
            65000   4
            70000   5
        >;
	};
};

&fiq_debugger{
	rockchip,baudrate = <115200>;  /* Only 115200 and 1500000 */
};

&pwm5 {
	pinctrl-0 = <&pwm5m1_pins>;
	status = "okay";
};

&i2c0 {

	status = "okay";

/* rtc */
	hym8563_1: hym8563_1@51 {
		compatible = "haoyu,hym8563";
		reg = <0x51>;
		#clock-cells = <0>;
		clock-frequency = <32768>;
		clock-output-names = "hym8563";
		pinctrl-names = "default";
		pinctrl-0 = <&hym8563_int>;
		interrupt-parent = <&gpio0>;
		interrupts = <RK_PB0 IRQ_TYPE_LEVEL_LOW>;
		wakeup-source;
		status = "okay";
	};

/* watch dog on core*/
    pc9202@3c {
        status = "disabled";
        compatible = "firefly,pc9202";
        reg = <0x3c>;
        wd-en-gpio = <&gpio2 RK_PC5 GPIO_ACTIVE_HIGH>;
    };
};

/* export pins */
&i2c5 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c5m0_xfer>;
};

&i2c6 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c6m0_xfer>;

/* watch dog on mother base*/
    pc9202_1@3c {
        status = "okay";
        compatible = "firefly,pc9202";
        reg = <0x3c>;
        wd-en-gpio = <&gpio3 RK_PC4 GPIO_ACTIVE_HIGH>;
    };
};

/* usb */
&vcc5v0_host{
	status = "okay";
	gpio = <&gpio4 RK_PC6 GPIO_ACTIVE_HIGH>;  
};
//up
&usbdrd_dwc3_0{
	/delete-property/ usb-role-switch;
	/delete-property/ dwc3_0_role_switch;
	dr_mode = "host";
};
//down
&usbdrd_dwc3_1{
	dr_mode = "host";
};

/* tf-card */
&sdmmc {
	status = "okay";
};

&vcc_sdcard_pwr_en{
	gpio = <&gpio1 RK_PA7 GPIO_ACTIVE_HIGH>; 
	status = "okay";
};

/* gamc0 */
&gmac0 {
	snps,reset-gpio = <&gpio4 RK_PA2 GPIO_ACTIVE_LOW>;
	tx_delay = <0x37>;
	status = "okay";
};

&gmac0_tx_bus2{
    rockchip,pins =
    /* gmac0_txd0 */
    <2 RK_PB6 1 &pcfg_pull_up_drv_level_6>,
    /* gmac0_txd1 */
    <2 RK_PB7 1 &pcfg_pull_up_drv_level_6>,
    /* gmac0_txen */
    <2 RK_PC0 1 &pcfg_pull_none>;
};

&gmac0_rgmii_bus{
	rockchip,pins =
	/* gmac0_rxd2 */
	<2 RK_PA6 1 &pcfg_pull_none>,
	/* gmac0_rxd3 */
	<2 RK_PA7 1 &pcfg_pull_none>,
	/* gmac0_txd2 */
	<2 RK_PB1 1 &pcfg_pull_up_drv_level_6>,
	/* gmac0_txd3 */
	<2 RK_PB2 1 &pcfg_pull_up_drv_level_6>;
};

/* gmac1 */
&gmac1 {
	snps,reset-gpio = <&gpio4 RK_PB5 GPIO_ACTIVE_LOW>;
	tx_delay = <0x47>;
	status = "okay";
};

&gmac1_tx_bus2{
	rockchip,pins =
	/* gmac1_txd0 */
	<3 RK_PB3 1 &pcfg_pull_up_drv_level_6>,
    /* gmac1_txd1 */
	<3 RK_PB4 1 &pcfg_pull_up_drv_level_6>,
	/* gmac1_txen */
	<3 RK_PB5 1 &pcfg_pull_none>;
};

&gmac1_rgmii_bus{
	rockchip,pins =
	/* gmac1_rxd2 */
	<3 RK_PA2 1 &pcfg_pull_none>,
	/* gmac1_rxd3 */
	<3 RK_PA3 1 &pcfg_pull_none>,
	/* gmac1_txd2 */
	<3 RK_PA0 1 &pcfg_pull_up_drv_level_6>,
	/* gmac1_txd3 */
	<3 RK_PA1 1 &pcfg_pull_up_drv_level_6>;
};

/* sata pm */
&combphy0_ps {
	status = "okay";
};


#if M2_SATA_OR_PCIE
&sata0 {
    //pinctrl-names = "default";
    //pinctrl-0 = <&sata_reset>;
	status = "okay";
};
#else
&pcie2x1l2 {
	reset-gpios = <&gpio4 RK_PC1 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc_sata_pwr_en>;
	status = "okay";
};
#endif

&vcc_sata_pwr_en{
	status = "okay";
	gpio = <&gpio0 RK_PD3 GPIO_ACTIVE_HIGH>;  
	regulator-min-microvolt = <3300000>;
	regulator-max-microvolt = <3300000>;
	regulator-always-on;
	enable-active-high;
	startup-delay-us = <5000>;
	vin-supply = <&vcc12v_dcin>;
};

/* uart1 for 232 */
&uart1{
	pinctrl-0 = <&uart1m1_xfer>;
	status = "okay";
};

/* uart6 for 485 and 485_RE_DE is GPIO4_C3_d*/
&uart6{
	pinctrl-0 = <&uart6m1_xfer>;
	status = "okay";
};

/* pinctrl */
&pinctrl {
	headphone {
		hp_det: hp-det {
			rockchip,pins = <1 RK_PC4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	leds {
		leds_gpio: leds-gpio {
			/* led_power */
			rockchip,pins = <3 16 RK_FUNC_GPIO &pcfg_pull_none>,
							<3 17 RK_FUNC_GPIO &pcfg_pull_none>,
							<2 19 RK_FUNC_GPIO &pcfg_pull_none>;
		};
 	};

 	hym8563 {
		hym8563_int: hym8563-int {
			rockchip,pins = <0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb-typec { 
		usbc0_int: usbc0-int { 
			rockchip,pins = <0 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>; 
		}; 
	};

	hdmirx {
		hdmirx_det: hdmirx-det {
			rockchip,pins = <1 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-bluetooth {
		uart6_gpios: uart6-gpios {
			rockchip,pins = <1 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_reset_gpio: bt-reset-gpio {
			rockchip,pins = <0 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_wake_gpio: bt-wake-gpio {
			rockchip,pins = <0 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_irq_gpio: bt-irq-gpio {
			rockchip,pins = <0 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-wlan {
		wifi_host_wake_irq: wifi-host-wake-irq {
			rockchip,pins = <0 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		wifi_poweren_gpio: wifi-poweren-gpio {
			rockchip,pins = <0 RK_PC4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};
