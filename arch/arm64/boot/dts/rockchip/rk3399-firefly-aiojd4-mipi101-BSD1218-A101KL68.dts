&i2c4 {
	status = "okay";

	hxchipset@48 {
        status = "disabled";
        compatible = "himax,hxcommon";
        reg = <0x48>;

        himax,rst-gpio =  <&gpio1 RK_PC7 GPIO_ACTIVE_HIGH>;
        himax,irq-gpio = <&gpio4 RK_PD4 IRQ_TYPE_LEVEL_LOW>;
        //himax,3v3-gpio = <&gpio0 RK_PB4 GPIO_ACTIVE_HIGH>;
        //himax,pon-gpio = <&gpio0 RK_PB4 GPIO_ACTIVE_HIGH>;

        himax,panel-coords = <0 800 0 1280>;      //触摸范围
        himax,display-coords = <0 800 0 1280>;    //分辨率
        report_type = <1>;
    };

    goodix_ts@14 {
        status = "okay";
        compatible = "goodix,gt9xxx,firefly";
        reg = <0x14>;
        interrupt-parent = <&gpio4>;
        interrupts = <RK_PD4 IRQ_TYPE_LEVEL_LOW>;
        //goodix,pwr-gpio = <&pca9555 PCA_IO0_7 GPIO_ACTIVE_HIGH>;
        reset-gpios = <&gpio1 RK_PD4 GPIO_ACTIVE_HIGH>;
        irq-gpios = <&gpio4 RK_PC1 IRQ_TYPE_LEVEL_LOW>;
        irq-flags = <2>; /* 1 rising, 2 falling */
        //goodix,swap-x2y = <1>;
        touchscreen-size-x = <800>;
        touchscreen-size-y = <1280>;
        goodix,slide-wakeup = <0>;
        goodix,type-a-report = <1>;
        goodix,driver-send-cfg = <1>;
        goodix,resume-in-workqueue = <0>;
        goodix,int-sync = <1>;
        //goodix,swap-x2y = <0>;
        goodix,esd-protect = <1>;
        goodix,auto-update-cfg = <1>;
        goodix,auto-update = <0>;
        goodix,power-off-sleep = <0>;
        goodix,pen-suppress-finger = <0>;
        goodix,has-pen = <0>;

